
<div class="parallax overflow-hidden bg-blue-400 page-section third">
    <div class="container parallax-layer" data-opacity="true">
        <div class="media v-middle">
            <div class="media-left text-center">
                <a href="#">
                    <img src="@Url.Content(ViewBag.UserData.ProfilePicPath)" alt="people" class="img-circle width-80" />
                </a>
            </div>
            <div class="media-body">
                <h1 class="text-white text-display-1 margin-v-0">
                    @ViewBag.UserData.StudentName
                </h1>
                @*<p class="text-subhead">@Html.ActionLink("View public profile", "PublicProfile", "Home", routeValues: new { CampusID = ViewBag.UserData.CampusID }, htmlAttributes: new { id = "signupLink", @class = "link-white text-underline" })</p>*@
            </div>
            <div class="media-right">
                <span class="label bg-blue-A100" style="font-size:larger">Student</span>
            </div>
        </div>
    </div>
</div>

