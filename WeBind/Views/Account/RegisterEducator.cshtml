    <div id="content">
        <fieldset class="step relative paper-shadow" style="margin-top:30px" data-z="2">

            <div class="container-fluid">
                <div class="tabbable tabs-blocks tabs-center-h tabs-primary">
                    <!-- Tabs -->
                    <!-- // END Tabs -->
                    <!-- Panes -->
                    <div class="tab-content">
                        <div id="expert-register" class="tab-pane active">
                            <div class="text-center paper-shadow" data-z="0.5">
                                <h1 class="text-display-1 text-center">Create an Expert account</h1>
                                <div class="panel-body" style="padding:0">
                                    <!-- Signup -->
                                    @{
                                        Model.RoleName = "Expert";
                                    }
                                    @using (Html.BeginForm("Register", "Account", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { role = "form" }))
                                    {
                                        @Html.AntiForgeryToken()

                                        @Html.HiddenFor(m => m.RoleName)
                                        <div class="form-group">
                                            <div class="form-control-material">
                                                @Html.TextBoxFor(m => m.FirstName, new { @class = "form-control text-center", @placeholder = "Email address", id = "exprtFirstName" })
                                                <label for="exprtFirstName">First Name</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-control-material">
                                                @Html.TextBoxFor(m => m.LastName, new { @class = "form-control text-center", @placeholder = "Last Name", id = "expertLastName" })
                                                <label for="expertLastName">Last Name</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-control-material">
                                                @Html.TextBoxFor(m => m.Email, new { @class = "form-control text-center", @placeholder = "Email address", id = "exprtemail" })
                                                <label for="expertemail">Email address</label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="form-control-material">
                                                @Html.PasswordFor(m => m.Password, new { @class = "form-control text-center", @placeholder = "Enter Password", id = "exprtpassword" })
                                                <label for="expertpassword">Password</label>
                                            </div>
                                        </div>
                                        <div class="form-group text-center">
                                            <div class="checkbox">
                                                <input type="checkbox" id="agree" />
                                                <label for="agree">* I Agree with <a href="#">Terms &amp; Conditions!</a></label>
                                            </div>
                                        </div>
                                        <div class="text-center">
                                            <button type="submit" class="btn btn-primary">Create An Account <i class="fa fa-fw fa-unlock-alt"></i></button>
                                        </div>
                                    }
                                    <!-- //Signup -->
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- // END Panes -->
                </div>
            </div>
        </fieldset>
    </div>


    <!-- Footer -->
    <footer class="footer">
        <strong>WeBind</strong> &copy; Copyright 2016
    </footer>
    <!-- // Footer -->
